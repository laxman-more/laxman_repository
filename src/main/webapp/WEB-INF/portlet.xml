<?xml version="1.0"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" 
	version="2.0">

	<portlet>
		<portlet-name>agent-registration-portlet</portlet-name>
		<display-name>agent-registration Portlet</display-name>
		<portlet-class>com.liferay.util.bridges.mvc.MVCPortlet</portlet-class>
		<init-param>
			<name>view-jsp</name>
			<value>/jsp/view.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
		</supports>
		<portlet-info>
			<title>agent-registration Portlet</title>
			<short-title>agent-registration Portlet</short-title>
			<keywords>agent-registration Portlet</keywords>
		</portlet-info>
	</portlet>
</portlet-app>